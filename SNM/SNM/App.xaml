<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="SNM.App"
	StartupUri="MainWindow.xaml">
	<Application.Resources>
		<!-- Debe definir aquí los recursos en el ámbito del nivel de aplicación. -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Visual/Scrollbar.xaml" />
                <ResourceDictionary Source="Visual/Tabitem.xaml" />
                <ResourceDictionary Source="Visual/Button.xaml" />
                <ResourceDictionary Source="Visual/Listbox.xaml" />
                <ResourceDictionary Source="Visual/Estilos.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>